ender: \"symbol\" },\n  },\n  {\n    title: \"WeekAvg\",\n    dataIndex: \"WeekAvg\",\n  },\n  {\n    title: \"lastPrice\",\n    dataIndex: \"lastPrice\",\n  },\n  {\n    title: \"ratio\",\n    dataIndex: \"ratio\",\n  },\n];\nconst now = new Date();\nexport default {\n  data() {\n    return {\n      data: [],\n      date: \"\" + now.getFullYear() + now.getMonth() + now.getDate(),\n      columns: columns,\n      chart: undefined,\n      tabs: [\n        {\n          key: \"7\",\n          tab: \"近一周\",\n        },\n        {\n          key: \"14\",\n          tab: \"近两周\",\n        },\n      ],\n    };\n  },\n  methods: {\n    changeTab(name) {\n      this.refresh(name);\n    },\n    refresh(choice) {\n      let that = this;\n      api.list(choice).then((res) => {\n        that.chart.source(res.data);\n        that.chart.render();\n      });\n    },\n    detail(date) {\n      api.detail(date).then((res) => {\n        this.data = res.data;\n      });\n    },\n    onChange(value, dateString) {\n      if (dateString) {\n        this.detail(dateString);\n      }\n    },\n  },\n  mounted() {\n    let containerRef = this.$refs[\"container\"];\n    let scrollWidth = containerRef.scrollWidth;\n    this.chart = new this.G2.Chart({\n      container: \"container\",\n      autoFit: true,\n      width: scrollWidth - 200,\n      height: 300,\n    });\n    this.chart.interval().position(\"date*sum\");\n    this.chart.tooltip({\n      showMarkers: false,\n    });\n    this.refresh(7);\n    this.detail(\"2020-12-17\");\n  },\n};\n</script>\n<style scoped>\n.ant-card-contain-tabs > .ant-card-head .ant-card-head-title {\n  font-size: 25px !important;\n}\n</style>\n"],"sourceRoot":""}]}